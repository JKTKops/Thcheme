;;; CONSTANTS
(define nil ())

;;; LIST FUNCTIONS
(define (map f xs)
  (if (null? xs)
      ()
      (cons (f (car xs))
            (map f (cdr xs)))))

(define (filter p xs)
  (if (null? xs)
      ()
      (if (p (car xs))
          (cons (car xs) (filter p (cdr xs)))
          (filter p (cdr xs)))))

